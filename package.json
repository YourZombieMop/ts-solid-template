{
  "name": "ts-solid-template",
  "version": "0.0.0",
  "description": "A template to build web applications with TypeScript, Solid, & Sass",
  "license": "MIT",
  "scripts": {
    "build": "yarn run build:dev",
    "build:dev": "webpack --config webpack/webpack.config.js --env env=dev",
    "build:prod": "webpack --config webpack/webpack.config.js --env env=prod",
    "start": "yarn run start:dev",
    "start:dev": "webpack serve --config webpack/webpack.config.js --env env=dev",
    "start:prod": "webpack serve --config webpack/webpack.config.js --env env=prod"
  },
  "dependencies": {
    "solid-js": "^1.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.19.0",
    "babel-loader": "^8.2.5",
    "babel-preset-solid": "^1.5.4",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "sass": "^1.54.8",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.1",
    "webpack-merge": "^5.8.0"
  }
}
